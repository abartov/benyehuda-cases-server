= form_for @team_membership do |f|
  - if @team_membership.errors.any?
    #error_explanation
      %h2= "#{pluralize(@team_membership.errors.count, "error")} prohibited this team_membership from being saved:"
      %ul
        - @team_membership.errors.full_messages.each do |message|
          %li= message

  .field
    = f.label :joined
    = f.datetime_select :joined
  .field
    = f.label :left
    = f.datetime_select :left
  .field
    = f.label :team_role
    = f.number_field :team_role
  .field
    = f.label :team_id
    = f.text_field :team_id
  .field
    = f.label :user_id
    = f.text_field :user_id
  .actions
    = f.submit 'Save'
