= form_for @task_team do |f|
  - if @task_team.errors.any?
    #error_explanation
      %h2= "#{pluralize(@task_team.errors.count, "error")} prohibited this task_team from being saved:"
      %ul
        - @task_team.errors.full_messages.each do |message|
          %li= message

  .field
    = f.label :task
    = f.text_field :task
  .field
    = f.label :team
    = f.text_field :team
  .actions
    = f.submit 'Save'
